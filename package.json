{
  "name": "shared-canvas",
  "version": "0.0.1",
  "description": "A shared canvas",
  "main": "./src/boot.ts",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js --open",
    "build": "NODE_ENV=production webpack-dev-server --config ./config/webpack.prod.js --open",
    "aot": "npm run compile && npm run build",
    "test": "karma start",
    "cleanup": "rimraf aot docs",
    "compile": "npm run cleanup && ngc",
    "e2e": "./node_modules/.bin/protractor",
    "wd:update": "./node_modules/protractor/bin/webdriver-manager update",
    "wd:start": "./node_modules/protractor/bin/webdriver-manager start"
  },
  "keywords": [
    "angular",
    "canvas",
    "drawing",
    "websocket"
  ],
  "author": "John Glukhov <jgluhov@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "core-js": "^2.5.4",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angularclass/hmr": "^2.1.3",
    "@types/core-js": "^0.9.46",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^9.6.2",
    "@types/webpack-env": "^1.13.5",
    "angular-router-loader": "^0.8.3",
    "angular2-template-loader": "git+https://github.com/jgluhov/angular2-template-loader.git",
    "awesome-typescript-loader": "^5.0.0",
    "codelyzer": "^4.2.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "null-loader": "^0.1.1",
    "protractor": "^5.3.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  }
}
